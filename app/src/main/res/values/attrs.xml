<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="AnimatedPathView">
        <attr name="strokeWidth" format="dimension" />
        <attr name="strokeColor" format="color|reference" />
        <attr name="fillColor" format="color|reference" />
        <attr name="phase" format="float" />
        <attr name="duration" format="integer" />
        <attr name="fillDuration" format="integer" />
        <attr name="fillOffset" format="integer" />
        <attr name="fadeFactor" format="float" />
        <attr name="riv_corner_radius" format="float" />
        <attr name="svgPath" format="reference" />
    </declare-styleable>

    <declare-styleable name="SpotlightView">
        <attr name="target" format="reference" />
        <attr name="mask" format="reference" />
    </declare-styleable>

    <declare-styleable name="BaseAppTheme">
        <attr name="actionBarIconColor" format="color" />
    </declare-styleable>
</resources>
